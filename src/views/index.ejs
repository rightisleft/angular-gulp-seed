<!DOCTYPE html>
<html ng-app="app">
  <head>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.0.0-alpha/css/bootstrap.css">
    <title>Jacks Test Playground</title>
    <!-- bower:js -->
    <script src="lib/jquery/dist/jquery.js"></script>
    <script src="lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="lib/angular/angular.js"></script>
    <script src="lib/angular-resource/angular-resource.js"></script>
    <script src="lib/angular-cookies/angular-cookies.js"></script>
    <script src="lib/angular-sanitize/angular-sanitize.js"></script>
    <script src="lib/angular-bootstrap/ui-bootstrap-tpls.js"></script>
    <script src="lib/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="lib/angular-breadcrumb/release/angular-breadcrumb.js"></script>
    <script src="lib/angular-backoff/dist/angular-backoff.min.js"></script>
    <script src="lib/jquery-ui/jquery-ui.js"></script>
    <script src="lib/angular-dragdrop/src/angular-dragdrop.js"></script>
    <script src="lib/angular-animate/angular-animate.js"></script>
    <script src="lib/angular-intercom/angular-intercom.js"></script>
    <script src="lib/angular-ui-tree/dist/angular-ui-tree.js"></script>
    <script src="lib/angular-ui-utils/jq.js"></script>
    <!-- endbower -->

    <!-- bower:css -->
    <link rel="stylesheet" href="lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="lib/font-awesome/css/font-awesome.min.css" />
    <link rel="stylesheet" href="lib/angular-ui-tree/dist/angular-ui-tree.min.css" />
    <!-- endbower -->

    <!-- inject:js -->
    <script src="/js/app.js"></script>
    <!-- endinject -->

    <!-- inject:css -->
    <link rel="stylesheet" href="/css/main.css">
    <!-- endinject -->
  </head>
  <body>
    <!-- This page uses EJS -->
    <!-- This page uses AngularJS 1.x -->
    <div class="container">
      <h1>Jacks Test Playground</h1>
      <div ui-view></div>
      <section id="main" class="container">
        <h1>Scoped Area</h1>
        <h2>Test: <%= list.length %></h2>
        <% for(var i=0; i < list.length; i++){ %>
          <li>Test: <%= list[i] %></li>
        <%}%>
      </section>
    </div>
  </body>
</html>